package jaxrs_helloservice;

import javax.ws.rs.*;

@ApplicationPath("example")
public class Application extends HelloWorldResource{

	@Override
	public Set<Class<?>> getClasses() {
		Set<Class<?>> classes = new HashSet<Class<?>>();
		classes.add(HelloWorldResource.class);
		// Aquí podemos añadir
		return classes;
	}
}
